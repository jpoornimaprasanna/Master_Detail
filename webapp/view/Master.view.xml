<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="demo.Masterdetails.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Master" id="height">
				<content>
					<List items="{fruits>/fruits}" itemPress="onPressItem" >
						<items type="active" >
							<ObjectListItem title="{fruits>/name}" type="Active" number="{fruits>price}" numberUnit="pc" icon="{fruits>image}" >
							</ObjectListItem>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>